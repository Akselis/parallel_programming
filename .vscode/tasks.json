{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build flpenum",
            "type": "shell",
            "command": "g++",
            "args": [
                "-O3",
                "-march=native",
                "-fopenmp",
                "-o", "flpenum.exe",
                "flpenum.cpp"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        }
    ]
}
